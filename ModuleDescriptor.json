{
  "id": "grails-helloworld-module",
  "name": "grails-hello-world",
  "provides": [
    {
      "id": "grails-hello-world",
      "version": "1.0",
      "handlers" : [
        {
          "methods": [ "GET" ],
          "pathPattern": "/hello"
        }
      ]
    },
